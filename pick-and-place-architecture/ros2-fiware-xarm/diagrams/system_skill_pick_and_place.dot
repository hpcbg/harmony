"Pick and Place" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Pick and Place", shape=box, style=filled];
Workflow -> "Pick and Place";
"Bottle Detected?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Bottle Detected?", shape=ellipse, style=filled];
"Pick and Place" -> "Bottle Detected?";
"Run Pick and Place" [fillcolor=gray, fontcolor=black, fontsize=9, label="Run Pick and Place", shape=ellipse, style=filled];
"Pick and Place" -> "Run Pick and Place";
"Wait 1 sec*" [fillcolor=gray, fontcolor=black, fontsize=9, label="Wait 1 sec*", shape=ellipse, style=filled];
"Pick and Place" -> "Wait 1 sec*";
"Finish System Skill" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Finish System Skill", shape=box, style=filled];
Workflow -> "Finish System Skill";
"Pick and Place Executed?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Pick and Place Executed?", shape=ellipse, style=filled];
