digraph pastafarianism {
ordering=out;
graph [fontname="times-roman"];
node [fontname="times-roman"];
edge [fontname="times-roman"];
"System Skill Pick" [fillcolor=gold, fontcolor=black, fontsize=9, label="System Skill Pick\nSuccessOnAll", shape=parallelogram, style=filled];
"System State" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ System State", shape=box, style=filled];
"System Skill Pick" -> "System State";
"Report Status" [fillcolor=gray, fontcolor=black, fontsize=9, label="Report Status", shape=ellipse, style=filled];
"System State" -> "Report Status";
"Robot Status" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot Status", shape=ellipse, style=filled];
"System State" -> "Robot Status";
"Bottle Detector Status" [fillcolor=gray, fontcolor=black, fontsize=9, label="Bottle Detector Status", shape=ellipse, style=filled];
"System State" -> "Bottle Detector Status";
Pick [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Pick", shape=box, style=filled];
"System Skill Pick" -> Pick;
"Active?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Active?", shape=ellipse, style=filled];
Pick -> "Active?";
"Move to Grasp Position" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Move to Grasp Position", shape=box, style=filled];
Pick -> "Move to Grasp Position";
"Bottle Detected?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Bottle Detected?", shape=ellipse, style=filled];
"Move to Grasp Position" -> "Bottle Detected?";
"Robot Stationary?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot Stationary?", shape=ellipse, style=filled];
"Move to Grasp Position" -> "Robot Stationary?";
"Release Gripper" [fillcolor=gray, fontcolor=black, fontsize=9, label="Release Gripper", shape=ellipse, style=filled];
"Move to Grasp Position" -> "Release Gripper";
"Wait 1 sec" [fillcolor=gray, fontcolor=black, fontsize=9, label="Wait 1 sec", shape=ellipse, style=filled];
"Move to Grasp Position" -> "Wait 1 sec";
"Robot Stationary?*" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot Stationary?*", shape=ellipse, style=filled];
"Move to Grasp Position" -> "Robot Stationary?*";
"Send Grasp Position" [fillcolor=gray, fontcolor=black, fontsize=9, label="Send Grasp Position", shape=ellipse, style=filled];
"Move to Grasp Position" -> "Send Grasp Position";
"Wait 1 sec*" [fillcolor=gray, fontcolor=black, fontsize=9, label="Wait 1 sec*", shape=ellipse, style=filled];
"Move to Grasp Position" -> "Wait 1 sec*";
Grasp [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Grasp", shape=box, style=filled];
Pick -> Grasp;
"Robot At Grasp Position?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot At Grasp Position?", shape=ellipse, style=filled];
Grasp -> "Robot At Grasp Position?";
"Close Gripper" [fillcolor=gray, fontcolor=black, fontsize=9, label="Close Gripper", shape=ellipse, style=filled];
Grasp -> "Close Gripper";
"Wait 1 sec**" [fillcolor=gray, fontcolor=black, fontsize=9, label="Wait 1 sec**", shape=ellipse, style=filled];
Grasp -> "Wait 1 sec**";
"Finish Pick" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Finish Pick", shape=box, style=filled];
Pick -> "Finish Pick";
"Robot Stationary" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot Stationary", shape=ellipse, style=filled];
"Finish Pick" -> "Robot Stationary";
"Finish Pick*" [fillcolor=gray, fontcolor=black, fontsize=9, label="Finish Pick*", shape=ellipse, style=filled];
"Finish Pick" -> "Finish Pick*";
}
