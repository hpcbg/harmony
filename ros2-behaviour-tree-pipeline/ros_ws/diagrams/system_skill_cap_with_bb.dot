"/detected_gesture" [color=blue, fillcolor=white, fixedsize=False, fontcolor=blue, fontsize=8, height=0, label="/detected_gesture: unknown", shape=box, style=filled, width=0];
"/detected_intent" [color=blue, fillcolor=white, fixedsize=False, fontcolor=blue, fontsize=8, height=0, label="/detected_intent: unknown", shape=box, style=filled, width=0];
"/robot_gripper" [color=blue, fillcolor=white, fixedsize=False, fontcolor=blue, fontsize=8, height=0, label="/robot_gripper: {'rotation': 0.0,...", shape=box, style=filled, width=0];
}

