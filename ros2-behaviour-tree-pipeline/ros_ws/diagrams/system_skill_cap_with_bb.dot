digraph pastafarianism {
ordering=out;
graph [fontname="times-roman"];
node [fontname="times-roman"];
edge [fontname="times-roman"];
"System Skill Cap" [fillcolor=gold, fontcolor=black, fontsize=9, label="System Skill Cap\nSuccessOnAll", shape=parallelogram, style=filled];
"System State" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ System State", shape=box, style=filled];
"System Skill Cap" -> "System State";
"Report Status" [fillcolor=gray, fontcolor=black, fontsize=9, label="Report Status", shape=ellipse, style=filled];
"System State" -> "Report Status";
"Robot Status" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot Status", shape=ellipse, style=filled];
"System State" -> "Robot Status";
"Gesture Detector Status" [fillcolor=gray, fontcolor=black, fontsize=9, label="Gesture Detector Status", shape=ellipse, style=filled];
"System State" -> "Gesture Detector Status";
"Human Intent Detector Status" [fillcolor=gray, fontcolor=black, fontsize=9, label="Human Intent Detector Status", shape=ellipse, style=filled];
"System State" -> "Human Intent Detector Status";
Cap [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Cap", shape=box, style=filled];
"System Skill Cap" -> Cap;
"Active?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Active?", shape=ellipse, style=filled];
Cap -> "Active?";
"Go to Cap Position" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Go to Cap Position", shape=box, style=filled];
Cap -> "Go to Cap Position";
"Robot Stationary?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot Stationary?", shape=ellipse, style=filled];
"Go to Cap Position" -> "Robot Stationary?";
"Send Cap Position" [fillcolor=gray, fontcolor=black, fontsize=9, label="Send Cap Position", shape=ellipse, style=filled];
"Go to Cap Position" -> "Send Cap Position";
"Wait 1 sec" [fillcolor=gray, fontcolor=black, fontsize=9, label="Wait 1 sec", shape=ellipse, style=filled];
"Go to Cap Position" -> "Wait 1 sec";
"Rotate Bottle" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Rotate Bottle", shape=box, style=filled];
Cap -> "Rotate Bottle";
"Robot at Cap Position?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot at Cap Position?", shape=ellipse, style=filled];
"Rotate Bottle" -> "Robot at Cap Position?";
Detectors [fillcolor=gold, fontcolor=black, fontsize=9, label="Detectors\nSuccessOnOne", shape=parallelogram, style=filled];
"Rotate Bottle" -> Detectors;
"Human Requested Rotation?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Human Requested Rotation?", shape=ellipse, style=filled];
Detectors -> "Human Requested Rotation?";
"Gesture Cap Holded Detected?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Gesture Cap Holded Detected?", shape=ellipse, style=filled];
Detectors -> "Gesture Cap Holded Detected?";
"Send Bottle Rotate Action" [fillcolor=gray, fontcolor=black, fontsize=9, label="Send Bottle Rotate Action", shape=ellipse, style=filled];
"Rotate Bottle" -> "Send Bottle Rotate Action";
"Wait 1 sec*" [fillcolor=gray, fontcolor=black, fontsize=9, label="Wait 1 sec*", shape=ellipse, style=filled];
"Rotate Bottle" -> "Wait 1 sec*";
"Finish Cap" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Finish Cap", shape=box, style=filled];
Cap -> "Finish Cap";
"Robot Stationary?*" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot Stationary?*", shape=ellipse, style=filled];
"Finish Cap" -> "Robot Stationary?*";
"Finish Cap*" [fillcolor=gray, fontcolor=black, fontsize=9, label="Finish Cap*", shape=ellipse, style=filled];
"Finish Cap" -> "Finish Cap*";
"/capping" -> "Report Status"  [color=green, constraint=False, weight=0];
"/capping" -> "Active?"  [color=green, constraint=False, weight=0];
"Finish Cap" -> "/capping"  [color=blue, constraint=False, weight=0];
"/robot_pose" -> "Robot Stationary?"  [color=green, constraint=False, weight=0];
"/robot_pose" -> "Robot at Cap Position?"  [color=green, constraint=False, weight=0];
"/robot_pose" -> "Robot Stationary?"  [color=green, constraint=False, weight=0];
"Robot Status" -> "/robot_pose"  [color=blue, constraint=False, weight=0];
"/robot_status" -> "Robot Stationary?"  [color=green, constraint=False, weight=0];
"/robot_status" -> "Robot at Cap Position?"  [color=green, constraint=False, weight=0];
"/robot_status" -> "Robot Stationary?"  [color=green, constraint=False, weight=0];
"Robot Status" -> "/robot_status"  [color=blue, constraint=False, weight=0];
"/detected_gesture" -> "Gesture Cap Holded Detected?"  [color=green, constraint=False, weight=0];
"Gesture Detector Status" -> "/detected_gesture"  [color=blue, constraint=False, weight=0];
"/detected_intent" -> "Human Requested Rotation?"  [color=green, constraint=False, weight=0];
"Human Intent Detector Status" -> "/detected_intent"  [color=blue, constraint=False, weight=0];
"/robot_gripper" -> "Robot Stationary?"  [color=green, constraint=False, weight=0];
"/robot_gripper" -> "Robot at Cap Position?"  [color=green, constraint=False, weight=0];
"/robot_gripper" -> "Robot Stationary?"  [color=green, constraint=False, weight=0];
"Robot Status" -> "/robot_gripper"  [color=blue, constraint=False, weight=0];
subgraph Blackboard {
id=Blackboard;
label=Blackboard;
rank=sink;
"Report Status" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Report Status", shape=ellipse, style=filled];
"Robot Status" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Robot Status", shape=ellipse, style=filled];
"Gesture Detector Status" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Gesture Detector Status", shape=ellipse, style=filled];
"Human Intent Detector Status" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Human Intent Detector Status", shape=ellipse, style=filled];
"Robot Stationary?" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Robot Stationary?", shape=ellipse, style=filled];
"Human Requested Rotation?" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Human Requested Rotation?", shape=ellipse, style=filled];
"Gesture Cap Holded Detected?" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Gesture Cap Holded Detected?", shape=ellipse, style=filled];
"Robot at Cap Position?" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Robot at Cap Position?", shape=ellipse, style=filled];
"Robot Stationary?" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Robot Stationary?", shape=ellipse, style=filled];
"Finish Cap" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Finish Cap", shape=ellipse, style=filled];
"Active?" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Active?", shape=ellipse, style=filled];
"/capping" [color=blue, fillcolor=white, fixedsize=False, fontcolor=blue, fontsize=8, height=0, label="/capping: -", shape=box, style=filled, width=0];
"/robot_pose" [color=blue, fillcolor=white, fixedsize=False, fontcolor=blue, fontsize=8, height=0, label="/robot_pose: {'x': 0, 'y': 0, ...", shape=box, style=filled, width=0];
