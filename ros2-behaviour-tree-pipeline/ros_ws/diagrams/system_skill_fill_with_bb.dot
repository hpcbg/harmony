digraph pastafarianism {
ordering=out;
graph [fontname="times-roman"];
node [fontname="times-roman"];
edge [fontname="times-roman"];
"System Skill Fill" [fillcolor=gold, fontcolor=black, fontsize=9, label="System Skill Fill\nSuccessOnAll", shape=parallelogram, style=filled];
"System State" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ System State", shape=box, style=filled];
"System Skill Fill" -> "System State";
"Report Status" [fillcolor=gray, fontcolor=black, fontsize=9, label="Report Status", shape=ellipse, style=filled];
"System State" -> "Report Status";
"Robot Status" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot Status", shape=ellipse, style=filled];
"System State" -> "Robot Status";
"Filling Station Status" [fillcolor=gray, fontcolor=black, fontsize=9, label="Filling Station Status", shape=ellipse, style=filled];
"System State" -> "Filling Station Status";
Fill [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Fill", shape=box, style=filled];
"System Skill Fill" -> Fill;
"Active?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Active?", shape=ellipse, style=filled];
Fill -> "Active?";
"Filling Station Ready?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Filling Station Ready?", shape=ellipse, style=filled];
Fill -> "Filling Station Ready?";
"Go to Filling Station" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Go to Filling Station", shape=box, style=filled];
Fill -> "Go to Filling Station";
"Robot Stationary?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot Stationary?", shape=ellipse, style=filled];
"Go to Filling Station" -> "Robot Stationary?";
"Send Filling Station Position" [fillcolor=gray, fontcolor=black, fontsize=9, label="Send Filling Station Position", shape=ellipse, style=filled];
"Go to Filling Station" -> "Send Filling Station Position";
"Wait 1 sec" [fillcolor=gray, fontcolor=black, fontsize=9, label="Wait 1 sec", shape=ellipse, style=filled];
"Go to Filling Station" -> "Wait 1 sec";
"Bottle Fill" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Bottle Fill", shape=box, style=filled];
Fill -> "Bottle Fill";
"Robot at Fill Position?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot at Fill Position?", shape=ellipse, style=filled];
"Bottle Fill" -> "Robot at Fill Position?";
"Send Fill Bottle Action" [fillcolor=gray, fontcolor=black, fontsize=9, label="Send Fill Bottle Action", shape=ellipse, style=filled];
"Bottle Fill" -> "Send Fill Bottle Action";
"Wait 1 sec*" [fillcolor=gray, fontcolor=black, fontsize=9, label="Wait 1 sec*", shape=ellipse, style=filled];
"Bottle Fill" -> "Wait 1 sec*";
"Finish Fill" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Finish Fill", shape=box, style=filled];
Fill -> "Finish Fill";
"Bottle Filled?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Bottle Filled?", shape=ellipse, style=filled];
"Finish Fill" -> "Bottle Filled?";
"Finish Fill*" [fillcolor=gray, fontcolor=black, fontsize=9, label="Finish Fill*", shape=ellipse, style=filled];
"Finish Fill" -> "Finish Fill*";
"/robot_status" -> "Robot Stationary?"  [color=green, constraint=False, weight=0];
"/robot_status" -> "Robot at Fill Position?"  [color=green, constraint=False, weight=0];
"Robot Status" -> "/robot_status"  [color=blue, constraint=False, weight=0];
"/fill_progress" -> "Bottle Filled?"  [color=green, constraint=False, weight=0];
"Filling Station Status" -> "/fill_progress"  [color=blue, constraint=False, weight=0];
"/filling" -> "Active?"  [color=green, constraint=False, weight=0];
"/filling" -> "Report Status"  [color=green, constraint=False, weight=0];
"Finish Fill" -> "/filling"  [color=blue, constraint=False, weight=0];
"/robot_gripper" -> "Robot Stationary?"  [color=green, constraint=False, weight=0];
"/robot_gripper" -> "Robot at Fill Position?"  [color=green, constraint=False, weight=0];
"Robot Status" -> "/robot_gripper"  [color=blue, constraint=False, weight=0];
"/robot_pose" -> "Robot Stationary?"  [color=green, constraint=False, weight=0];
"/robot_pose" -> "Robot at Fill Position?"  [color=green, constraint=False, weight=0];
"Robot Status" -> "/robot_pose"  [color=blue, constraint=False, weight=0];
"/status" -> "Filling Station Ready?"  [color=green, constraint=False, weight=0];
"Filling Station Status" -> "/status"  [color=blue, constraint=False, weight=0];
subgraph Blackboard {
id=Blackboard;
label=Blackboard;
rank=sink;
"Report Status" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Report Status", shape=ellipse, style=filled];
"Robot Status" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Robot Status", shape=ellipse, style=filled];
"Filling Station Status" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Filling Station Status", shape=ellipse, style=filled];
"Robot Stationary?" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Robot Stationary?", shape=ellipse, style=filled];
"Robot at Fill Position?" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Robot at Fill Position?", shape=ellipse, style=filled];
"Bottle Filled?" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Bottle Filled?", shape=ellipse, style=filled];
"Finish Fill" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Finish Fill", shape=ellipse, style=filled];
"Active?" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Active?", shape=ellipse, style=filled];
"Filling Station Ready?" [color=blue, fillcolor=gray, fontcolor=blue, fontsize=7, label="Filling Station Ready?", shape=ellipse, style=filled];
"/robot_status" [color=blue, fillcolor=white, fixedsize=False, fontcolor=blue, fontsize=8, height=0, label="/robot_status: {'status': 'busy'}", shape=box, style=filled, width=0];
"/fill_progress" [color=blue, fillcolor=white, fixedsize=False, fontcolor=blue, fontsize=8, height=0, label="/fill_progress: 100", shape=box, style=filled, width=0];
"/filling" [color=blue, fillcolor=white, fixedsize=False, fontcolor=blue, fontsize=8, height=0, label="/filling: -", shape=box, style=filled, width=0];
"/robot_gripper" [color=blue, fillcolor=white, fixedsize=False, fontcolor=blue, fontsize=8, height=0, label="/robot_gripper: {'rotation': 0.0,...", shape=box, style=filled, width=0];
"/robot_pose" [color=blue, fillcolor=white, fixedsize=False, fontcolor=blue, fontsize=8, height=0, label="/robot_pose: {'x': 0, 'y': 0, ...", shape=box, style=filled, width=0];
"/status" [color=blue, fillcolor=white, fixedsize=False, fontcolor=blue, fontsize=8, height=0, label="/status: wait", shape=box, style=filled, width=0];
}

}
