digraph pastafarianism {
ordering=out;
graph [fontname="times-roman"];
node [fontname="times-roman"];
edge [fontname="times-roman"];
"System Skill Handover" [fillcolor=gold, fontcolor=black, fontsize=9, label="System Skill Handover\nSuccessOnAll", shape=parallelogram, style=filled];
"System State" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ System State", shape=box, style=filled];
"System Skill Handover" -> "System State";
"Report Status" [fillcolor=gray, fontcolor=black, fontsize=9, label="Report Status", shape=ellipse, style=filled];
"System State" -> "Report Status";
"Robot Status" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot Status", shape=ellipse, style=filled];
"System State" -> "Robot Status";
"Gesture Detector Status" [fillcolor=gray, fontcolor=black, fontsize=9, label="Gesture Detector Status", shape=ellipse, style=filled];
"System State" -> "Gesture Detector Status";
"Human Intent Detector Status" [fillcolor=gray, fontcolor=black, fontsize=9, label="Human Intent Detector Status", shape=ellipse, style=filled];
"System State" -> "Human Intent Detector Status";
Handover [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Handover", shape=box, style=filled];
"System Skill Handover" -> Handover;
"Active?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Active?", shape=ellipse, style=filled];
Handover -> "Active?";
"Go to Handover Position" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Go to Handover Position", shape=box, style=filled];
Handover -> "Go to Handover Position";
"Robot Stationary" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot Stationary", shape=ellipse, style=filled];
"Go to Handover Position" -> "Robot Stationary";
"Send Handover Position" [fillcolor=gray, fontcolor=black, fontsize=9, label="Send Handover Position", shape=ellipse, style=filled];
"Go to Handover Position" -> "Send Handover Position";
"Wait 1 sec" [fillcolor=gray, fontcolor=black, fontsize=9, label="Wait 1 sec", shape=ellipse, style=filled];
"Go to Handover Position" -> "Wait 1 sec";
"Execute Handover" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Execute Handover", shape=box, style=filled];
Handover -> "Execute Handover";
"Robot at Handover Position?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot at Handover Position?", shape=ellipse, style=filled];
"Execute Handover" -> "Robot at Handover Position?";
Detectors [fillcolor=gold, fontcolor=black, fontsize=9, label="Detectors\nSuccessOnOne", shape=parallelogram, style=filled];
"Execute Handover" -> Detectors;
"Human Requested Bottle Release?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Human Requested Bottle Release?", shape=ellipse, style=filled];
Detectors -> "Human Requested Bottle Release?";
"Gesture Bottle Holded?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Gesture Bottle Holded?", shape=ellipse, style=filled];
Detectors -> "Gesture Bottle Holded?";
"Release Bottle" [fillcolor=gray, fontcolor=black, fontsize=9, label="Release Bottle", shape=ellipse, style=filled];
"Execute Handover" -> "Release Bottle";
"Wait 1 sec*" [fillcolor=gray, fontcolor=black, fontsize=9, label="Wait 1 sec*", shape=ellipse, style=filled];
"Execute Handover" -> "Wait 1 sec*";
"Robot Stationary?" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot Stationary?", shape=ellipse, style=filled];
"Execute Handover" -> "Robot Stationary?";
"Finish Handover" [fillcolor=orange, fontcolor=black, fontsize=9, label="Ⓜ Finish Handover", shape=box, style=filled];
Handover -> "Finish Handover";
"Reset Gripper Rotation" [fillcolor=gray, fontcolor=black, fontsize=9, label="Reset Gripper Rotation", shape=ellipse, style=filled];
"Finish Handover" -> "Reset Gripper Rotation";
"Set Home Position" [fillcolor=gray, fontcolor=black, fontsize=9, label="Set Home Position", shape=ellipse, style=filled];
"Finish Handover" -> "Set Home Position";
"Wait 1 sec**" [fillcolor=gray, fontcolor=black, fontsize=9, label="Wait 1 sec**", shape=ellipse, style=filled];
"Finish Handover" -> "Wait 1 sec**";
"Robot Stationary?*" [fillcolor=gray, fontcolor=black, fontsize=9, label="Robot Stationary?*", shape=ellipse, style=filled];
"Finish Handover" -> "Robot Stationary?*";
"Finish Handover*" [fillcolor=gray, fontcolor=black, fontsize=9, label="Finish Handover*", shape=ellipse, style=filled];
"Finish Handover" -> "Finish Handover*";
}
